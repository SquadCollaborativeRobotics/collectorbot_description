<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="Collection_bot">
    <static>false</static>

    <!-- //////////// -->
    <link name="base_link">
      <pose>0 0 0.138 0 0 0</pose>
      <inertial>
        <mass>2.0</mass>
        <inertia>
          <ixx>.25</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>.25</iyy>
          <iyz>0.0</iyz>
          <izz>.25</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.6096 0.4572 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.6096 0.4572 0.1</size>
          </box>
        </geometry>
        <material>
          <ambient>0.6 0.6 1 1</ambient>
          <diffuse>0.6 0.6 1 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>

      <collision name="caster_collision">
        <pose>-0.254 0 -0.06905 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.06905</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="caster_visual">
        <pose>-0.254 0 -0.06905 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.06905</radius>
          </sphere>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>
    </link>
    
    <link name="left_wheel_link">
      <pose>0.2413 -0.2536 0.0619125 1.57079632679 0 0</pose>
      <inertial>
        <mass>.143</mass>
        <inertia>
          <ixx>.00041955</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>.00027407</iyy>
          <iyz>0.0</iyz>
          <izz>.00041955</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>
    </link>

    <link name="right_wheel_link">
      <pose>0.2413 0.2536 0.0619125 1.57079632679 0 0</pose>
      <inertial>
        <mass>.143</mass>
        <inertia>
          <ixx>.00041955</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>.00027407</iyy>
          <iyz>0.0</iyz>
          <izz>.00041955</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>
    </link>

    <!-- Joints -->
    <joint name="base_left_wheel" type="revolute">
      <parent>base_link</parent>
      <child>left_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint name="base_right_wheel" type="revolute">
      <parent>base_link</parent>
      <child>right_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <include>
      <uri>model://hokuyo</uri>
      <pose>0.25 0 .2 0 0 0</pose>
    </include>
    <joint name="hokuyo_joint" type="revolute">
      <child>hokuyo::link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>

  <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>20</updateRate>
      <leftJoint>base_left_wheel</leftJoint>
      <rightJoint>base_right_wheel</rightJoint>
      <wheelSeparation>0.5072</wheelSeparation>
      <wheelDiameter>0.123825</wheelDiameter>
      <torque>20</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>base_link</robotBaseFrame>
    </plugin>

  </model>
</sdf>
