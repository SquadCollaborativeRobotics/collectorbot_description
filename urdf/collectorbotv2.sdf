<?xml version='1.0'?>
<sdf version='1.4'>
  <model name="Collection_bot">
    <static>false</static>

    <!-- //////////// -->
    <link name="base_link">
      <pose>0 0 0.138 0 0 0</pose>
      <inertial>
        <mass>2.0</mass>
        <inertia>
          <ixx>.25</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>.25</iyy>
          <iyz>0.0</iyz>
          <izz>.25</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.4572 0.3556 0.1</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <box>
            <size>0.4572 0.3556 0.1</size>
          </box>
        </geometry>
        <material>
          <ambient>0.6 0.6 1 1</ambient>
          <diffuse>0.6 0.6 1 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>

      <collision name="trash_can">
        <pose>-0.114 0 0.1 0 0 0</pose>
        <geometry>
          <box>
            <size>0.2 0.3556 0.2</size>
          </box>
        </geometry>
      </collision>
      <visual name="trash_visual">
        <pose>-0.114 0 0.1 0 0 0</pose>
        <geometry>
          <box>
            <size>.2 .3556 .2</size>
          </box>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>

      <collision name="caster_collision">
        <pose>-0.154 0 -0.06905 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.06905</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="caster_visual">
        <pose>-0.154 0 -0.06905 0 0 0</pose>
        <geometry>
          <sphere>
            <radius>0.06905</radius>
          </sphere>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>
    </link>

    <link name="left_wheel_link">
      <pose>0.1413 -0.1778 0.0619125 1.57079632679 0 0</pose>
      <inertial>
        <mass>.143</mass>
        <inertia>
          <ixx>.00041955</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>.00027407</iyy>
          <iyz>0.0</iyz>
          <izz>.00041955</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>
    </link>

    <link name="right_wheel_link">
      <pose>0.1413 0.1778 0.0619125 1.57079632679 0 0</pose>
      <inertial>
        <mass>.143</mass>
        <inertia>
          <ixx>.00041955</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>.00027407</iyy>
          <iyz>0.0</iyz>
          <izz>.00041955</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <length>0.05</length>
            <radius>0.0619125</radius>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>
    </link>

    <!-- Joints -->
    <joint name="base_left_wheel" type="revolute">
      <parent>base_link</parent>
      <child>left_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint name="base_right_wheel" type="revolute">
      <parent>base_link</parent>
      <child>right_wheel_link</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <link name="hokuyo_link">
      <pose>0.15 0 .2 0 0 0</pose>
      <inertial>
        <mass>0.1</mass>
      </inertial>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://hokuyo/meshes/hokuyo.dae</uri>
          </mesh>
        </geometry>
      </visual>

      <collision name="collision-base">
        <pose>0 0 -0.0145 0 0 0</pose>
        <geometry>
          <box>
            <size>0.05 0.05 0.041</size>
          </box>
        </geometry>
      </collision>

      <collision name="collision-top">
        <pose>0 0 0.0205 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.021</radius>
            <length>0.029</length>
          </cylinder>
        </geometry>
      </collision>


      <sensor name="laser" type="ray">
        <pose>0.01 0 0.0175 0 -0 0</pose>
        <ray>
          <scan>
            <horizontal>
              <samples>640</samples>
              <resolution>1</resolution>
              <min_angle>-2.26889</min_angle>
              <max_angle>2.268899</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.08</min>
            <max>10</max>
            <resolution>0.01</resolution>
          </range>
        </ray>

        <plugin name="laser" filename="libgazebo_ros_laser.so">
          <frameName>hokuyo_frame</frameName>
          <topicName>scan</topicName>
        </plugin>
        <always_on>1</always_on>
        <update_rate>30</update_rate>
        <visualize>true</visualize>
      </sensor>
    </link>

    <joint name="camera_joint" type="revolute">
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
      <!-- <origin><xyz>0.1 0 0.2></xyz><rpy>0 0 0</rpy></origin> -->
      <parent>base_link</parent>
      <child>camera_link</child>
    </joint>

    <!-- Camera -->
    <link name="camera_link">
      <collision name="collision-camera">
        <!-- <pose>0 0 0.4 0 0 0</pose> -->
        <pose>0.25 0 .25 0 0 0</pose>

        <geometry>
          <box><size>.1 .1 0.1</size></box>
        </geometry>
      </collision>

      <visual name="visual-camera">
        <!-- <pose>0 0 0.4 0 0 0</pose> -->

        <pose>0.25 0 .25 0 0 0</pose>
        <!-- <origin><xyz>0 0 0</xyz><rpy>0 0 0</rpy></origin> -->
        <geometry>
          <box><size>.1 .1 0.1</size></box>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.3 0.3 0.3 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
          <emissive>0 0 0 0</emissive>
        </material>
      </visual>

      <inertial>
        <mass>1e-5</mass>
        <!-- <origin><xyz>0 0 0</xyz><rpy>0 0 0</rpy></origin> -->
        <inertia>
          <ixx>1e-6</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>1e-6</iyy>
          <iyz>0</iyz>
          <izz>1e-6</izz>
        </inertia>
      </inertial>

      <sensor type="camera" name="camera1">
        <update_rate>30.0</update_rate>
        <camera name="head">
          <horizontal_fov>1.3962634</horizontal_fov>
          <image>
            <width>800</width>
            <height>800</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.02</near>
            <far>300</far>
          </clip>
          <noise>
            <type>gaussian</type>
            <mean>0.0</mean>
            <stddev>0.007</stddev>
          </noise>
        </camera>
        <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
          <alwaysOn>true</alwaysOn>
          <updateRate>0.0</updateRate>
          <cameraName>rrbot/camera1</cameraName>
          <imageTopicName>image_raw</imageTopicName>
          <cameraInfoTopicName>camera_info</cameraInfoTopicName>
          <frameName>camera_link</frameName>
          <hackBaseline>0.07</hackBaseline>
          <distortionK1>0.0</distortionK1>
          <distortionK2>0.0</distortionK2>
          <distortionK3>0.0</distortionK3>
          <distortionT1>0.0</distortionT1>
          <distortionT2>0.0</distortionT2>
        </plugin>
      </sensor>
    </link>

    <joint name="hokuyo_joint" type="revolute">
      <child>hokuyo_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>

    <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>20</updateRate>
      <leftJoint>base_left_wheel</leftJoint>
      <rightJoint>base_right_wheel</rightJoint>
      <wheelSeparation>0.5072</wheelSeparation>
      <wheelDiameter>0.123825</wheelDiameter>
      <torque>20</torque>
      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom</odometryTopic>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>robot_center</robotBaseFrame>
    </plugin>

  </model>
</sdf>
